<!DOCTYPE html>
<html>
<head>
  <title>Meetup</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <link rel="stylesheet" href="css/common.css" type="text/css">
  <link rel="stylesheet" href="css/index.css" type="text/css">
  <script src="http://code.jquery.com/jquery-1.11.0.min.js" type="text/javascript" charset="utf-8"></script>
  <script src="http://code.jquery.com/jquery-migrate-1.2.1.min.js" type="text/javascript" charset="utf-8"></script>
  <script src="js/jquery.anystretch.min.js" type="text/javascript" charset="utf-8"></script>
</head>
<body class="<% params[:controller] + '-' + params[:action] %>">
    <div class="navbar clearfix">
      <div class="container">
        <a class="navbar-brand" href="/">
          Meetup
        </a>
        <ul class="nav left">
          <li><%= link_to "About", "/about" %></li>
          <li><a href="#">placeholder</a></li>
        </ul>
        <ul class="nav right">
          <li><a href="#">login</a></li>
          <li><a href="#">signup</a></li>
        </ul>
      </div>
    </div>

<%= yield %>

    <div class="footer">
      <div class="container">
        footer
      </div>
    </div>

    <script>
      $('.home-banner').anystretch();
    </script>
</body>
</html>
